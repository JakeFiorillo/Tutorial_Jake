// room script file

function room_LeaveRight()
{
  cEgo.ChangeRoom(2, 10, 130);  
}

function oBucket_Look(Object *theObject, CursorMode mode)
{
  Display("There seems to be an empty bucket under that rock");
}

function oBucket_Interact(Object *theObject, CursorMode mode)
{
  cEgo.Walk(40, 125, eBlock, eWalkableAreas);
  cEgo.FaceDirection(eDirectionDown);
  oBucket.Visible=false;
  Display("You picket up the bucket");
  cEgo.AddInventory(iBucket);
}
